<?xml version="1.0" encoding="UTF-8" ?>
<odoo noupdate="1">

<!--    Adjustment Sequence-->
    <record id="adjustment_seq" model="ir.sequence">
        <field name="name">adjustment.seq</field>
        <field name="code">adjustment.seq</field>
        <field name="prefix">CAA</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

<!--    Money Transfer Sequence-->
    <record id="mt_seq" model="ir.sequence">
        <field name="name">money.transfer.seq</field>
        <field name="code">money.transfer.seq</field>
        <field name="prefix">MT</field>
        <field name="padding">3</field>
        <field name="company_id" eval="False"/>
    </record>

